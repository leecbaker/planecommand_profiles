{
    "$schema": "https://planecommand.com/schema/aircraft_profile_v3.1.json",
    "name": "Zibo 737-800X",
    "name_filter": "Boeing 737-800X",
    "author": "SSmith",
    "author_filter": "Unruh",
    "config": {
        "flaps": {
            "settings": [
                0,
                1,
                2,
                5,
                10,
                15,
                25,
                30,
                40
            ],
            "response": "degree"
        },
        "taxi_light": {
            "status": "laminar/B738/toggle_switch/taxi_light_brightness_pos",
            "on": [
                "laminar/B738/toggle_switch/taxi_light_brightness_pos_dn",
                "laminar/B738/toggle_switch/taxi_light_brightness_pos_dn"
            ],
            "off": [
                "laminar/B738/toggle_switch/taxi_light_brightness_pos_up",
                "laminar/B738/toggle_switch/taxi_light_brightness_pos_up"
            ]
        },
        "landing_light": {
            "status": "sim/cockpit2/switches/landing_lights_on",
            "on": "sim/lights/landing_lights_on",
            "off": [
                "sim/lights/landing_lights_off",
                "laminar/B738/switch/land_lights_left_pos=0",
                "laminar/B738/switch/land_lights_right_pos=0"
            ]
        },
        "beacon_light": {
            "status": "sim/cockpit2/switches/beacon_on",
            "on": "sim/lights/beacon_lights_on",
            "off": "sim/lights/beacon_lights_off"
        },
        "strobe_light": {
            "status": "sim/cockpit/electrical/strobe_lights_on",
            "on": [
                "laminar/B738/toggle_switch/position_light_up",
                "laminar/B738/toggle_switch/position_light_up"
            ],
            "off": "laminar/B738/toggle_switch/position_light_down"
        },
        "autopilot": {
            "status": "laminar/B738/autopilot/cmd_a_status",
            "on": "laminar/B738/autopilot/cmd_a_press",
            "off": "laminar/B738/autopilot/disconnect_toggle"
        },
        "flight_director": {
            "status": "laminar/B738/autopilot/flight_direct_on",
            "on": [
                "laminar/B738/autopilot/flight_director_toggle",
                "laminar/B738/autopilot/flight_director_fo_toggle"
            ],
            "off": [
                "laminar/B738/autopilot/flight_director_toggle",
                "laminar/B738/autopilot/flight_director_fo_toggle"
            ]
        },
        "autothrottle": {
            "status": "laminar/B738/autopilot/autothrottle_status",
            "on": "laminar/B738/autopilot/autothrottle_arm_toggle",
            "off": "laminar/B738/autopilot/autothrottle_arm_toggle"
        },
        "ap_altitude": {
            "value": "laminar/B738/autopilot/mcp_alt_dial",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "laminar/B738/autopilot/altitude_up",
                    "down": "laminar/B738/autopilot/altitude_dn",
                    "step": 100
                }
            ]
        },
        "ap_heading": {
            "value": "sim/cockpit2/autopilot/heading_dial_deg_mag_pilot",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "laminar/B738/autopilot/heading_up",
                    "down": "laminar/B738/autopilot/heading_dn",
                    "step": 1
                }
            ]
        },
        "ap_airspeed_knots": {
            "value": "sim/cockpit2/autopilot/airspeed_dial_kts",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "sim/autopilot/airspeed_up",
                    "down": "sim/autopilot/airspeed_down",
                    "step": 1
                }
            ]
        },
        "obs1": {
            "value": "sim/cockpit2/radios/actuators/nav1_obs_deg_mag_pilot",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "laminar/B738/autopilot/course_pilot_up",
                    "down": "laminar/B738/autopilot/course_pilot_dn",
                    "step": 1
                }
            ]
        },
        "obs2": {
            "value": "sim/cockpit2/radios/actuators/nav2_obs_deg_mag_copilot",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "laminar/B738/autopilot/course_copilot_up",
                    "down": "laminar/B738/autopilot/course_copilot_dn",
                    "step": 1
                }
            ]
        },
        "ap_vnav": {
            "status": "laminar/B738/autopilot/vnav_status1",
            "on": "laminar/B738/autopilot/vnav_press",
            "off": "laminar/B738/autopilot/vnav_press"
        },
        "ap_hnav": {
            "status": "laminar/B738/autopilot/lnav_status",
            "on": "laminar/B738/autopilot/lnav_press",
            "off": "laminar/B738/autopilot/lnav_press"
        },
        "ap_approach_hold": {
            "status": "laminar/B738/autopilot/app_status",
            "on": "laminar/B738/autopilot/app_press",
            "off": "laminar/B738/autopilot/app_press"
        },
        "ap_alt_hold": {
            "status": "laminar/B738/autopilot/alt_hld_status",
            "on": "laminar/B738/autopilot/alt_hld_press",
            "off": "laminar/B738/autopilot/alt_hld_press"
        },
        "ap_speed_hold": {
            "status": "laminar/B738/autopilot/speed_status1",
            "on": "laminar/B738/autopilot/speed_press",
            "off": "laminar/B738/autopilot/speed_press"
        },
        "ap_level_change": {
            "status": "laminar/B738/autopilot/lvl_chg_status",
            "on": "laminar/B738/autopilot/lvl_chg_press",
            "off": "laminar/B738/autopilot/lvl_chg_press"
        },
        "ap_heading_hold": {
            "status": "laminar/B738/autopilot/hdg_sel_status",
            "on": "laminar/B738/autopilot/hdg_sel_press",
            "off": "laminar/B738/autopilot/hdg_sel_press"
        },
        "ap_vert_speed_hold": {
            "status": "laminar/B738/autopilot/vs_status",
            "on": "laminar/B738/autopilot/vs_press",
            "off": "laminar/B738/autopilot/vs_press"
        },
        "ap_vspeed": {
            "value": "sim/cockpit/autopilot/vertical_velocity",
            "value_step": 1,
            "adjustments": [
                {
                    "up": "sim/autopilot/vertical_speed_up",
                    "down": "sim/autopilot/vertical_speed_down",
                    "step": 1
                }
            ]
        },
        "altimeter": {
            "value": "sim/cockpit/misc/barometer_setting",
            "value_step": 100,
            "adjustments": [
                {
                    "up": "laminar/B738/pilot/barometer_up",
                    "down": "laminar/B738/pilot/barometer_down"
                }
            ]
        }
    }
}