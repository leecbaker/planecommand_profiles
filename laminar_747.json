{
    "$schema": "https://planecommand.com/schema/aircraft_profile_v3.1.json",
    "name": "XP11 default 747-400",
    "author": "Lee C Baker",
    "name_filter": "Boeing 747-400",
    "author_filter": "Gazzawi",
    "config": {
        "flaps": {
            "response": "degree"
        },
        "autopilot": {
            "status": "laminar/B747/autopilot/cmd_L_mode/status", 
            "on": "laminar/B747/autopilot/button_switch/cmd_L",
            "off": "laminar/B747/autopilot/slide_switch/disengage_bar"
        },
        "flight_director": {
            "status": "laminar/B747/autopilot/AFDS/status_annun", 
            "on": "laminar/B747/toggle_switch/flight_dir_L",
            "off": "laminar/B747/toggle_switch/flight_dir_L"
        },
        "autothrottle": {
            "status": "laminar/B747/autothrottle/armed", 
            "on": "laminar/B747/toggle_switch/autothrottle",
            "off": "laminar/B747/toggle_switch/autothrottle"
        },
        "landing_light": {
            "status": "sim/cockpit/electrical/landing_lights_on",
            "on": ["laminar/B747/toggle_switch/landing_light_IBL", "laminar/B747/toggle_switch/landing_light_IBR", "laminar/B747/toggle_switch/landing_light_OBL", "laminar/B747/toggle_switch/landing_light_OBR"],
            "off": ["laminar/B747/toggle_switch/landing_light_IBL", "laminar/B747/toggle_switch/landing_light_IBR", "laminar/B747/toggle_switch/landing_light_OBL", "laminar/B747/toggle_switch/landing_light_OBR"]
        },
        "taxi_light": {
            "status": "sim/cockpit/electrical/taxi_light_on",
            "on": "laminar/B747/toggle_switch/taxi_light",
            "off": "laminar/B747/toggle_switch/taxi_light"
        },
        "strobe_light": {
            "status": "sim/cockpit2/switches/strobe_lights_on",
            "on": "laminar/B747/toggle_switch/strobe_light",
            "off": "laminar/B747/toggle_switch/strobe_light"
        },
        "beacon_light": {
            "status": "sim/cockpit2/switches/beacon_on",
            "on": "laminar/B747/toggle_switch/beacon_light_up",
            "off": "laminar/B747/toggle_switch/beacon_light_down"
        },
        "nav_light": {
            "status": "sim/cockpit2/switches/navigation_lights_on",
            "on": "laminar/B747/toggle_switch/nav_light",
            "off": "laminar/B747/toggle_switch/nav_light"
        }
    }
}